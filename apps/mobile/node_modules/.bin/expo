#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/studio/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__react-native@0.7_xjuliryibzpbc3ochkzartpr7y/node_modules/expo/bin/node_modules:/home/user/studio/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__react-native@0.7_xjuliryibzpbc3ochkzartpr7y/node_modules/expo/node_modules:/home/user/studio/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__react-native@0.7_xjuliryibzpbc3ochkzartpr7y/node_modules:/home/user/studio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/studio/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__react-native@0.7_xjuliryibzpbc3ochkzartpr7y/node_modules/expo/bin/node_modules:/home/user/studio/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__react-native@0.7_xjuliryibzpbc3ochkzartpr7y/node_modules/expo/node_modules:/home/user/studio/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__react-native@0.7_xjuliryibzpbc3ochkzartpr7y/node_modules:/home/user/studio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../expo/bin/cli" "$@"
else
  exec node  "$basedir/../expo/bin/cli" "$@"
fi
