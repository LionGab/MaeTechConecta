{
  "name": "maetech-conecta-monorepo",
  "private": true,
  "packageManager": "npm@10.8.2",
  "scripts": {
    "dev": "turbo dev --filter=web -- \"$@\"",
    "dev:all": "turbo dev",
    "dev:mobile": "turbo dev --filter=mobile",
    "start": "pnpm -r start",
    "build": "turbo build",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "install:all": "npm install"
  },
  "devDependencies": {
    "turbo": "^2.0.9",
    "typescript": "^5.5.4",
    "prettier": "^3.3.3"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-safe-area-context": "4.10.5",
    "typescript": "~5.3.3"
  }
}
