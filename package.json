{
  "name": "nossa-maternidade",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "pnpm -C apps/mobile start",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "e2e:android": "maestro test e2e/maestro/smoke-flow.yaml",
    "audit": "pnpm audit --audit-level=moderate || true",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.4.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@sentry/react-native": "^7.5.0",
    "@stripe/stripe-react-native": "0.38.6",
    "@supabase/supabase-js": "^2.48.0",
    "@types/react": "~18.3.12",
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "expo-av": "~15.0.2",
    "expo-notifications": "~0.29.14",
    "expo-status-bar": "~2.0.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.8",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-safe-area-context": "4.12.0",
    "turbo": "^2.1.0",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "overrides": {
      "@types/react": "~18.3.12",
      "react": "18.3.1"
    }
  }
}
