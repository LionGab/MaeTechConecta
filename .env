# ClubNath VIP - Environment Variables
# Copy this file to .env and fill in your actual values

# ========================================
# SUPABASE CONFIGURATION
# ========================================
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# ========================================
# EXTERNAL APIs
# ========================================

# OLLIN Babytest Integration
VITE_OLLIN_API_KEY=your_ollin_api_key
VITE_OLLIN_API_BASE_URL=https://api.babytest.com.br

# Push Notifications (Firebase/OneSignal)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# OneSignal (Alternative to Firebase)
VITE_ONESIGNAL_APP_ID=your_onesignal_app_id
VITE_ONESIGNAL_REST_API_KEY=your_onesignal_rest_api_key

# ========================================
# AR/3D SERVICES
# ========================================

# AR Try-On Service
VITE_AR_API_KEY=your_ar_api_key
VITE_AR_SERVICE_URL=https://api.ar-tryon.com

# 3D Model Service
VITE_3D_API_KEY=your_3d_api_key
VITE_3D_SERVICE_URL=https://api.3d-models.com

# ========================================
# SAFETY & LOCATION SERVICES
# ========================================

# Google Maps API (for location services)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Emergency Services API
VITE_EMERGENCY_API_KEY=your_emergency_api_key
VITE_EMERGENCY_SERVICE_URL=https://api.emergency-services.com

# ========================================
# ANALYTICS & MONITORING
# ========================================

# Google Analytics
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Sentry (Error Tracking)
VITE_SENTRY_DSN=your_sentry_dsn
VITE_SENTRY_ENVIRONMENT=production

# LogRocket (Session Replay)
VITE_LOGROCKET_APP_ID=your_logrocket_app_id

# ========================================
# PAYMENT & E-COMMERCE
# ========================================

# Stripe (Payment Processing)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key
VITE_STRIPE_SECRET_KEY=sk_test_your_stripe_secret

# PayPal
VITE_PAYPAL_CLIENT_ID=your_paypal_client_id

# ========================================
# AI & MACHINE LEARNING
# ========================================

# OpenAI (for NathIA)
VITE_OPENAI_API_KEY=your_openai_api_key

# Claude AI (Alternative)
VITE_CLAUDE_API_KEY=your_claude_api_key

# ========================================
# SOCIAL LOGIN
# ========================================

# Google OAuth
VITE_GOOGLE_CLIENT_ID=your_google_client_id

# Apple OAuth
VITE_APPLE_CLIENT_ID=your_apple_client_id

# Instagram OAuth
VITE_INSTAGRAM_CLIENT_ID=your_instagram_client_id

# ========================================
# FEATURE FLAGS
# ========================================

# Enable/Disable Features
VITE_ENABLE_SAFETY_FEATURES=true
VITE_ENABLE_AR_TRYON=true
VITE_ENABLE_BABYTEST_INTEGRATION=true
VITE_ENABLE_ADVANCED_MATCHING=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=true

# ========================================
# DEVELOPMENT & DEBUGGING
# ========================================

# Development Mode
VITE_DEV_MODE=false
VITE_DEBUG_MODE=false

# Mock Data
VITE_USE_MOCK_DATA=true

